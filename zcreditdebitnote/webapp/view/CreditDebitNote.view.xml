<mvc:View controllerName="zcreditdebitnote.controller.CreditDebitNote"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
    <Page id="page" title="{i18n>title}">
        <content>
            <Panel backgroundDesign="Transparent" class="myBackgroundStyle">
                <content>
                    <f:SimpleForm editable="true" emptySpanL="6" emptySpanXL="5" emptySpanM="5" layout="ResponsiveGridLayout" class=""  >
                        <f:content>
                            <Label text="Company Code" ></Label>
                            <Input id="idComCode" width="300px" maxLength="4"></Input>
                            <Label text="Document Number"></Label>
                            <!-- <HBox id="BH"> -->
                            <!-- <Input id="idDocNum" width="100px" maxLength="10"></Input> -->
                            <!-- <Input id="idDocNum1" width="100px" maxLength="10"></Input> -->
                            <!-- </HBox> -->
                            <MultiInput width="300px" id="idDocNum" maxLength="10" showValueHelp="false"></MultiInput>
                            <Label text="Fiscal Year"></Label>
                            <DatePicker width="250px" valueFormat="yyyy" displayFormat="yyyy" id="idDate"></DatePicker>
                            <!-- <Label text="Plant"></Label>
                            <Input id="idPlant" maxLength="4"></Input> -->
                            <Label id="lab"></Label>
                            <Button width="200px" icon="sap-icon://print" text="Print" type="Emphasized" press="onSinglePrint"></Button>
                        </f:content>
                    </f:SimpleForm>
                </content>
            </Panel>
        </content>
        <!-- <footer>
            <Bar>
                <contentLeft>
                    <Button icon="sap-icon://print" text="Print" press="onSubmit" />
                </contentLeft>
            </Bar>
        </footer> -->
    </Page>
</mvc:View>
